<div class="container">
    <div class="rectangle">
      {{#projects}}
      <div class="clip-rectangle clip-rectangle--{{project.id}}" style="background-image: url('../img/{{project.id}}.png');"></div>
      {{/projects}}
      <!-- <div class="clip-rectangle clip-rectangle--heaven clip-rectangle--hide-right" style="background-image: url('../img/h-copie.png');"></div> -->
    </div>
    <svg>
    <defs>
      <clipPath id="svgPath">
        <rect class="rect1" stroke="#000000" stroke-miterlimit="10"/>
        <rect class="rect2" stroke="#000000" stroke-miterlimit="10"/>
        <rect class="rect3" stroke="#000000" stroke-miterlimit="10"/>
      </clipPath>
     </defs>
   </svg>
</div>
{{#projects}}
    <div class="container__title">
        <div class="container__title__project container__title__project--{{project.id}}">
            <div class="container__title--flex container__title--{{project.id}}">
              <h2>{{project.name}}</h2>
              <h3>{{project.agency}}</h3>
              <div class="container__button--open container__button--{{project.id}}">
                <p>See the project</p>
              </div>
            </div>
        </div>
    </div>
{{/projects}}
